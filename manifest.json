{
  "manifest_version": 2,
  "name": "kjax-all-reload",
  "version": "1.0",
  "description": "You can reload all tabs from context menu.",

  "background": {
    "scripts": ["main.js"]
  },
  "permissions": [
    "menus",
    "tabs"
  ],
  "default_locale": "en",
  "homepage_url": "https://github.com/kjaxdev/tab-reload-all"
}
